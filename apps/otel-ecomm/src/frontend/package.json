{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run grpc:generate && NODE_OPTIONS='-r ./utils/telemetry/Instrumentation.js' next dev",
    "build": "next build",
    "start": "node --require ./Instrumentation.js server.js",
    "lint": "next lint",
    "cy:open": "cypress open",
    "grpc:generate": "mkdir -p ./protos && protoc -I ./pb  --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./protos --ts_proto_opt=outputServices=grpc-js demo.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.8.12",
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/auto-instrumentations-node": "0.36.4",
    "@opentelemetry/auto-instrumentations-web": "0.31.2",
    "@opentelemetry/context-zone": "1.10.1",
    "@opentelemetry/core": "1.10.1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.36.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.36.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.36.1",
    "@opentelemetry/instrumentation": "0.36.1",
    "@opentelemetry/resources": "1.10.1",
    "@opentelemetry/sdk-metrics": "1.10.1",
    "@opentelemetry/sdk-node": "0.36.1",
    "@opentelemetry/sdk-trace-base": "1.10.1",
    "@opentelemetry/sdk-trace-node": "1.10.1",
    "@opentelemetry/sdk-trace-web": "1.10.1",
    "@opentelemetry/resource-detector-alibaba-cloud": "0.27.4",
    "@opentelemetry/resource-detector-aws": "1.2.2",
    "@opentelemetry/resource-detector-container": "0.2.2",
    "@opentelemetry/resource-detector-gcp": "0.28.0",
    "cookies-next": "^2.1.1",
    "currency-symbol-map": "5.1.0",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@tanstack/react-query": "4.28.0",
    "sharp": "0.31.3",
    "styled-components": "5.3.9",
    "ts-proto": "1.143.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "9.0.1",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "cypress": "12.12.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "openapi-typescript": "6.2.0",
    "typescript": "5.0.2"
  }
}
